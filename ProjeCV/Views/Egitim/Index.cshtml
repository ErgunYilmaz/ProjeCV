@model ProjeCV.Models.Sınıf.Class1
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>BAŞLIK</th>
            <th>ALT BAŞLIK</th>
            <th>BÖLÜM</th>
            <th>GNO</th>
            <th>SİL</th>
            <th>GÜNCELLE</th>
            <th>ACTİONLİNK</th> 

        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.Deger3)
        {
        <tr>
            <th>@x.ID</th>
            <td>@x.TİTLE</td>
            <td>@x.SUBTİTLE</td>
            <td>@x.DEPARTMAN</td>
            <td>@x.GPA</td>
            <td><a href="/Egitim/Sil/@x.ID" class="btn btn-danger">SİL</a></td>
            <td><a href="/Egitim/EgitimGetir/@x.ID" class="btn btn-success">GÜNCELLE</a></td>
            <td>
                @Html.ActionLink("Sil Butonu ", "Sil", new { id = x.ID },
          new { @class = "btn btn-warning", onclick = "return confirm ('Silmek İstiyor Musunuz?')" })
            </td>
        </tr>
        }
    </tbody>

</table>
<a href="/Egitim/YeniEgitim/" class="btn btn-info">Yeni Eğitim Ekle</a>       

