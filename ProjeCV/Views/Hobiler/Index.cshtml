@model ProjeCV.Models.Sınıf.Class1
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>HOBİ</th>
            <th>SİL</th>
            <th>GÜNCELLE</th>
            <th>ACTİONLİNK</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.Deger5)
        {
        <tr>
            <th>@x.ID</th>
            <td>@x.İNTEREST</td>
            <td><a href="/Hobiler/Sil/@x.ID" class="btn btn-danger">SİL</a></td>
            <td><a href="/Hobiler/HobiGetir/@x.ID" class="btn btn-success">GÜNCELLE</a></td>
            <td>
                @Html.ActionLink("Sil Butonu ", "Sil", new { id = x.ID },
          new { @class = "btn btn-warning", onclick = "return confirm ('Silmek İstiyor Musunuz?')" })
        </td>
        </tr>
        }
    </tbody>

</table>
<a href="/Hobiler/YeniHobi/" class="btn btn-info">Yeni Hobi Ekle</a>



