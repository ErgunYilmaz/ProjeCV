@model ProjeCV.Models.Sınıf.Class1
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>AD</th>
            <th>SOYAD</th>
            <th>ADRES</th>
            <th>TELEFON</th>
            <th>MAİL</th>
            <th>HAKKIMDA</th>
            <th>Güncelle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.Deger1)
        {
        <tr>
            <th>@x.ID</th>
            <td>@x.NAME</td>
            <td>@x.SURNAME</td>
            <td>@x.ADDRESS</td>
            <td>@x.PHONE</td>
            <td>@x.MAİL</td>
            <td>@x.ABOUT</td>
            <td><a href="/Hakkımda/VeriGetir/@x.ID" class="btn btn-success">GÜNCELLE</a></td>

        </tr>
        }
    </tbody>
</table>


